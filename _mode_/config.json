{
	"default": {
		"default":{
			"template": "default.html",
			"title": "King",
			"curIndex": "current"
		},
		"solution/*.html":{
			"template": "default.html",
			"title": "King",
			"curSolution": "current"
		},
		"kingstatic/*.html":{
			"template": "kingstatic.html",
			"title": "King Static",
			"curIndex": "current"
		},
		"kingstatic/module/*.html":{
			"template": "kingstatic.html",
			"title": "King Static",
			"curModule": "current"
		}
	},
	"page": {
		"king.html": {
			"css": "/static/css/king/king.css",
			"curIndex": "",
			"curKing": "current"
		},
		"solution.html":{
			"curIndex": "",
			"curSolution": "current"
		},
		"kingstatic/style.html":{
			"curIndex": "",
			"curStyle": "current"
		},
		"kingstatic/module.html":{
			"curIndex": "",
			"curModule": "current",
			"css": "/static/css/kingstatic/module.css"
		},
		"kingstatic/png.html":{
			"curIndex": "",
			"curPng": "current"
		},
		"kingstatic/module/jwplayer.html":{
			"js": ["/static/jwplayer/jwplayer.js", "/static/js/kingstatic/jwplayer.js"]
		},
		"kingstatic/module/ckplayer.html":{
			"js": ["/static/ckplayer/ckplayer_clear.js", "/static/js/kingstatic/ckplayer.js"]
		},
		"kingstatic/module/wmvplayer.html":{
			"js": ["/static/wmvplayer/silverlight.js", "/static/wmvplayer/wmvplayer.js", "/static/js/kingstatic/wmvplayer.js"]
		},
